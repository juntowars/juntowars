{
  "name": "juntowars",
  "description": "juntowars",
  "keywords": [
    "express",
    "mongoose",
    "mongodb",
    "passport",
    "demo"
  ],
  "version": "4.0.0",
  "private": false,
  "author": "junto developement",
  "repository": {
    "type": "git",
    "url": "https://github.com/juntowars/juntowars"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "start": "NODE_PATH=./config:./app/controllers NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "test_env": "NODE_PATH=./config:./app/controllers NODE_ENV=test ./node_modules/.bin/nodemon server.js",
    "nightwatch_single_user": "/usr/local/bin/node nightwatch.js --group test/nightwatch/single_user -c nightwatch.json",
    "nightwatch_full_user_list": "/usr/local/bin/node nightwatch.js --group test/nightwatch/full_user_list -c nightwatch.json --env chrome,chrome,chrome,chrome,chrome,chrome",
    "pretest": "mongo test test/support/build_users.js",
    "test": "eval 'npm run test_env & npm run nightwatch_single_user && npm run nightwatch_full_user_list' || npm run posttest",
    "posttest": "mongo test test/support/tear_down_users.js & npm run stop",
    "stop": "killall -2 node"
  },
  "dependencies": {
    "async": "1.2.1",
    "bluebird": "2.9.30",
    "body-parser": "1.12.4",
    "compression": "1.5.0",
    "connect-flash": "0.1.1",
    "connect-mongo": "0.8.1",
    "cookie-parser": "1.3.5",
    "cookie-session": "1.1.0",
    "csurf": "1.8.3",
    "express": "^5.0.0-alpha.1",
    "express-session": "1.11.3",
    "forever": "0.14.1",
    "imager": "^0.4.0",
    "jquery": "2.1.4",
    "jsdom": "3.1.2",
    "method-override": "2.3.3",
    "mongoose": "^4.0.2",
    "morgan": "1.5.3",
    "multer": "0.1.8",
    "notifier": "0.1.7",
    "passport": "0.2.2",
    "passport-local": "1.0.0",
    "socket.io": "1.3.5",
    "swig": "1.4.2",
    "view-helpers": "0.1.5",
    "winston": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "*",
    "cucumber": "latest",
    "nodemon": "*",
    "should": "*",
    "nightwatch": "0.7.6",
    "supertest": "*"
  }
}
